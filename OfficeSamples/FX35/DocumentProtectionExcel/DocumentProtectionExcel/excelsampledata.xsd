<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="ExcelSampleDataSchema" targetNamespace="http://tempuri.org/ExcelSampleDataSchema.xsd"
xmlns="http://tempuri.org/ExcelSampleDataSchema.xsd"
xmlns:mstns="http://tempuri.org/ExcelSampleDataSchema.xsd"
xmlns:xs="http://www.w3.org/2001/XMLSchema" 
xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" 
attributeFormDefault="qualified" elementFormDefault="qualified" >
  <xs:element name="CustomersDataSet" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="Customer" msdata:Locale="en-US">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="firstName" type="xs:string" minOccurs="0" />
              <xs:element name="lastName" type="xs:string" minOccurs="0" />
	      <xs:element name="userName" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>
